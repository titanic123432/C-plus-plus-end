#include <iostream>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) 
{
	int n,m,a,b,c,i,j;
	int car[100][100];
	scanf("%d",&n);
	scanf("%d",&m);
	for(i = 0; i < m; i++)
	{
		int from,to,cnt;
		scanf("%d %d %d",&from,&to,cnt);
		car[from][to] = 1;
	}
	for(i = 0; i < n; i++)
	{
		scanf("%d %d",&i,&j);
		car[i][j] = 1;
	}
	return 0;
}
