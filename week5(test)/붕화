#include <iostream>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) 
{
	int n,m[100],ex = 0;
	scanf("%d",&n);
	for(int i = 0; i < n; i++)
	{
		scanf("%d",&m[i]);
	}
	for(int j = n; j >= 0; j--)
	{
		if(m[j - 1] < m[j - 2])
		{
			ex = ex + 1;
		}
		
	}
	printf("%d",ex);
	return 0;
}
