	scanf("%d %d %d %d",&n1, &n2, &n3, &n4);
	sum3 = n1 + n2 + n3 + n4;
	if(sum1 == 3)
	{
		printf("A\n");
	}
	else if(sum1 == 2)
	{
		printf("B\n");
	}
	else if(sum1 == 1)
	{
		printf("C\n");
	}
	else if(sum1 == 0)
	{
		printf("D\n");
	}
	else if(sum1 == 4)
	{
		printf("E\n");
	}
	//------------------
